#include<iostream>
#include<cstring>
#include<set>
using namespace std;
void printChars(int *s,int no,set<int> & myS){
	int j = 0,sum = 0;
	for(;no>0;no = no>>1){
	     int last_bit = (no&1);
	     if(last_bit==1){
    		sum+=s[j];
	      }
	      if(no==1){
	          myS.insert(sum);
	      }
	   j++;
	}
}

void subsetGen(int *s,int n,set<int> & myS){
	for(int i=0; i < (1<<n); i++){
		printChars(s,i,myS);
	}
}
int main(){
	int in[100000];
	int n,i,testCases;
	set<int> mySet;
	set<int>::iterator it;
	cin>>testCases;
	while(testCases){
	    cin>>n;
	    for(i=0;i<n;i++){
	        cin>>in[i];
	    }	  
	    mySet.clear();
	    subsetGen(in,n,mySet);
    	it = mySet.end();
    	cout<<*--it;
    	cout<<endl;
        --testCases;
	}
	return 0;
}
